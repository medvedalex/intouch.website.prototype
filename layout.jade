doctype html
html
	head
		block title
		link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous")
		link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous")
		link(rel="stylesheet" href="stylesheets/custom.css")
		

	body
		nav.navbar.navbar-inverse.navbar-fixed-top
			.container
				.navbar-header
					button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					//a.navbar-brand(href="index.html") img(src="logo.jpg")
					img.navbar-brand(src="pictures/logo.jpg")
				div#navbar.collapse.navbar-collapse
					mixin list(active)
						ul.nav.navbar-nav
							li(class=(active=="home"?"active":""))
								a(href="index.html") Home
							li(class=(active=="about"?"active":""))
								a(href="about.html") About
							li(class=(active=="mission"?"active":""))
								a(href="mission.html") Mission
							li(class=(active=="science"?"active":""))
								a(href="science.html") Science
							li(class=(active=="press"?"active":""))
								a(href="press.html") Press
							li(class=(active=="blog"?"active":""))
								a(href="blog.html") Blog
							li(class=(active=="contacts"?"active":""))
								a(href="contacts.html") Contacts
					block menuItems

		.container
			.starter-template
				block content
		
		script(src="https://code.jquery.com/jquery-3.2.1.min.js")
		script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous")
		
		
